{
  "name": "infurarelay",
  "version": "1.0.0",
  "description": "A Node/Express.js-based web server wrapping the functionality of the Infura API standard HTTPS Web3 methods.",
  "main": "app.js",
  "scripts": {
    "test": "babel-node test/index.js",
    "start": "pm2 start dist/app.js -i max",
    "clean": "rimraf dist",
    "logs": "pm2 logs",
    "stop": "pm2 delete all",
    "restart": "pm2 reload all",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "dev": "nodemon src/app.js --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/theAlexPatin/InfuraRelay.git"
  },
  "keywords": [
    "Infura",
    "Ethereum",
    "Blockchain",
    "Decentralized",
    "Express.js",
    "Node",
    "API Relay"
  ],
  "author": "Squarelink, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theAlexPatin/InfuraRelay/issues"
  },
  "homepage": "https://github.com/theAlexPatin/InfuraRelay#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.18.2",
    "celebrate": "^8.0.0",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "express-sanitizer": "^1.0.4",
    "helmet": "^3.14.0",
    "method-override": "^2.3.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.0.20",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.1.4",
    "babel-preset-stage-0": "^6.0.15",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^4.0.1",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2"
  }
}
